{
  "name": "ask",
  "version": "0.0.1",
  "description": "Quick and easy to generate a cli",
  "main": "index.js",
  "repository": "https://github.com/blackcater/ask.git",
  "author": "blackcater <blackcater2015@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "rollup -c rollup.config.js -w",
    "pro": "rollup -c rollup.config.js",
    "lint": "xo",
    "test": "jest --color --clearMocks",
    "test:watch": "jest --color --clearMocks --watch",
    "coverage": "jest --color --clearMocks --coverage"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "cli-spinners": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint-config-finger": "^0.2.0",
    "jest": "^20.0.4",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-json": "^2.1.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "xo": "^0.18.2"
  },
  "xo": {
    "extends": [
      "finger"
    ],
    "rules": {
      "quotes": [
        "error",
        "single"
      ]
    },
    "envs": [
      "node",
      "es6",
      "jest"
    ]
  }
}
